    "description": "Makes use of Shoot in an HTTP context more convenient",
    "type": "library",
    "keywords": [
        "shoot",
        "http"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Welling",
            "email": "victorwelling@gmail.com"
        }
    ],
    "abandoned": "shoot/shoot",
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "php": "^7.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "shoot/shoot": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
